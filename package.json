{
  "name": "rtd-style",
  "version": "0.5.6",
  "description": "shared style assets for RTD",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:css": "stylelint \"scss/**/*.scss\"",
    "format": "prettier --write \"**/*.{scss,json,yml,md,js}\"",
    "build:dev": "webpack -d --mode development",
    "build": "webpack -p --mode production",
    "start": "webpack-dev-server --mode development"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:css"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rideRTD/rtd-style.git"
  },
  "author": "RTD",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rideRTD/rtd-style/issues"
  },
  "homepage": "https://github.com/rideRTD/rtd-style#readme",
  "devDependencies": {
    "css-loader": "4.2.1",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.2.5",
    "img-loader": "3.0.1",
    "mini-css-extract-plugin": "0.10.0",
    "node-sass": "4.12.0",
    "prettier": "2.0.5",
    "resolve-url-loader": "3.1.1",
    "sass-loader": "7.3.1",
    "style-loader": "1.2.1",
    "stylelint": "13.6.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-prettier": "1.1.2",
    "stylelint-scss": "3.18.0",
    "svg-url-loader": "6.0.0",
    "tar": ">=4.4.10",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-fix-style-only-entries": "0.5.1"
  }
}
